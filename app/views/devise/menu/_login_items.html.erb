<% if consultant_signed_in? %>
    <%= link_to 'Edit Profile', edit_consultant_path(current_consultant.employee_id), :class => 'btn btn-primary' %>
    <%= link_to 'Sign out', destroy_consultant_session_path, :class => 'btn btn-primary' %>
<% else %>
    <%= link_to 'Sign in', new_consultant_session_path, :class => 'btn btn-primary' %>
<% end %>